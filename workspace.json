                    "executor": "@nrwl/jest:jest",
                    "outputs": ["coverage/apps/meerkat"],
                    "options": {
                        "jestConfig": "apps/meerkat/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "tags": []
        },
