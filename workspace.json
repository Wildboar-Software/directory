                        "tsConfig": "libs/idm/tsconfig.lib.json",
                        "packageJson": "libs/idm/package.json",
                        "main": "libs/idm/src/index.ts",
                        "assets": ["libs/idm/*.md"]
                    }
                },
                "lint": {
                    "executor": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/idm/**/*.ts"]
