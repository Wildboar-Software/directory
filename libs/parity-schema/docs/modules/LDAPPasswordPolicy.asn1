	LDAP-NAME 					{"pwdFailureCountInterval"}
    ID                          id-at-pwdFailureCountInterval
}

pwdMustChange ATTRIBUTE ::= {
    WITH SYNTAX 				BOOLEAN
    EQUALITY MATCHING RULE 		booleanMatch
