	LDAP-SYNTAX 				boolean.&id
	LDAP-NAME 					{"pwdMustChange"}
    ID                          id-at-pwdMustChange
}

pwdAllowUserChange ATTRIBUTE ::= {
    WITH SYNTAX 				BOOLEAN
    EQUALITY MATCHING RULE 		booleanMatch
    SINGLE VALUE                TRUE
	LDAP-SYNTAX 				boolean.&id
	LDAP-NAME 					{"pwdAllowUserChange"}
    ID                          id-at-pwdAllowUserChange
